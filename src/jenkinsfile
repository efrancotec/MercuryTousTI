pipeline{
    agent { label 'master'}

    stages{
        stage ('test'){
            steps{
                dir("build_java"){
                    sh "mvn test clean"
                }
            }
        }
        stage ('Build application'){
            steps{
                echo "Mvn clean install"
            }
        }
        stage ('create docker image'){
            steps{
                echo "Creando docker"
            }
        }
    }
}
